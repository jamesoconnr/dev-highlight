{
    "manifest_version": 3,
    "name": "Dev Highlight",
    "description": "Highlight/label localhost tabs",
    "version": "1.0",
    "content_scripts": [
        {
          "js": ["main.js"],
          "matches": [
            "https://localhost:*/*",
            "http://localhost:*/*"
          ]
        }
      ]
  }